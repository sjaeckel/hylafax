#
# To pick up defs from somewhere else override KITDEFS on the command line
#
KITDEFS=${ROOT}/usr/include/make/commondefs
include ${KITDEFS}

TIFF=	../libtiff
UTIL=	../util
OPTIMIZER=-O

BIN=	/usr/local/bin/fax
SPOOL=	/usr/spool/fax
ETC=	/usr/etc
FAXUSER=fax

LC++OPTS=+a1 +w +p -float
LCOPTS=	-float

#if CYPRESS_XGL
LCOPTS+=-cckr
LC++DEFS=-DCYPRESS_XGL
LC++OPTS+=-cckr
LIBFXUI=libfxui_xgl
WSLIB=	-lX11
#else
LIBFXUI=libfxui
WSLIB=	-lcps
#endif
