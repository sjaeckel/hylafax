DEPTH=..
@MAKEINCLUDE@ @MAKELQUOTE@${DEPTH}/defs@MAKERQUOTE@

SRCDIR	= ${DEPTH}/${TOPSRCDIR}/libtiff
VPATH	= ../@SRCDIR@/libtiff

LCOPTS=	@LIBTIFF_CONFIG@
#
CFILES=\
	tif_fax3.c \
	tif_fax4.c \
	tif_aux.c \
	tif_ccittrle.c \
	tif_close.c \
	tif_compress.c \
	tif_dir.c \
	tif_dirinfo.c \
	tif_dirread.c \
	tif_dirwrite.c \
	tif_dumpmode.c \
	tif_error.c \
	tif_getimage.c \
	tif_jpeg.c \
	tif_flush.c \
	tif_lzw.c \
	tif_next.c \
	tif_open.c \
	tif_packbits.c \
	tif_print.c \
	tif_read.c \
	tif_swab.c \
	tif_strip.c \
	tif_thunder.c \
	tif_tile.c \
	tif_unix.c \
	tif_version.c \
	tif_warning.c \
	tif_write.c
TARGETS=libtiff.${DSO}

LDIRT=	g3states.h mkg3states

default all::
	@${MAKE} incdepend
	@${MAKE} targets

targets: $(TARGETS)

@MAKEINCLUDE@ @MAKELQUOTE@${COMMONRULES}@MAKERQUOTE@

libtiff.a: ${OBJECTS}
	@rm -f $@;
	@echo "${AR} ${AROPTS} $@ ${OBJECTS}"; ${AR} ${AROPTS} $@ ${OBJECTS}
	${RANLIB} $@

tif_fax3.o: g3states.h
incdepend: g3states.h

g3states.h: mkg3states.c t4.h
	${CC} -o mkg3states ${CFLAGS} ${SRCDIR}/mkg3states.c
	./mkg3states -c > g3states.h

install: default

@MAKEDSOINCLUDE@ @MAKELQUOTE@${SRCDIR}/Makefile.dso@MAKERQUOTE@
